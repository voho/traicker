{
  "name": "traicker",
  "description": "A modern full-stack template powered by Cloudflare Workers, using Hono for backend APIs, React Router for frontend routing, and shadcn/ui for beautiful, accessible components styled with Tailwind CSS",
  "private": true,
  "type": "module",
  "cloudflare": {
    "label": "React Router + Hono Fullstack App",
    "products": [
      "Workers"
    ],
    "categories": [
      "starter"
    ],
    "docs_url": "https://developers.cloudflare.com/workers",
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/cd71c67a-253f-477d-022c-2f90cb4b3d00/public",
    "icon_urls": [],
    "publish": true
  },
  "dependencies": {
    "@clerk/backend": "^2.7.0",
    "@clerk/clerk-react": "^5.40.0",
    "@clerk/react-router": "^1.8.11",
    "@hono/clerk-auth": "^3.0.3",
    "@libsql/kysely-libsql": "^0.4.1",
    "@tanstack/react-query": "^5.84.2",
    "hono": "^4.8.2",
    "isbot": "5.1.26",
    "kysely": "^0.28.5",
    "kysely-codegen": "^0.18.5",
    "kysely-d1": "^0.4.0",
    "openai": "^5.12.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-is": "^19.1.1",
    "react-router": "7.6.3",
    "recharts": "^3.1.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.7.5",
    "@cloudflare/workers-types": "^4.20250425.0",
    "@react-router/dev": "7.6.3",
    "@tailwindcss/vite": "4.1.4",
    "@types/node": "24.0.4",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "tailwindcss": "4.1.1",
    "typescript": "5.8.3",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "5.1.4",
    "wrangler": "4.21.x"
  },
  "scripts": {
    "build": "react-router build",
    "types": "wrangler types",
    "db:manage": "npx d1s",
    "db:migrate:create": "wrangler d1 migrations create db-traicker-dev $1",
    "db:migrate:local": "wrangler d1 migrations apply --local db-traicker-dev",
    "db:migrate:remote": "wrangler d1 migrations apply --remote db-traicker-dev",
    "db:types": "npx kysely-codegen",
    "deploy": "npm run build && wrangler deploy",
    "dev": "react-router dev",
    "preview": "npm run build && vite preview",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b"
  }
}
